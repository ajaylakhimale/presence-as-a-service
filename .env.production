# Production Environment Variables
# Copy this file to .env.production and fill in your actual production values

# Supabase Configuration (REQUIRED)
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Domain Configuration (REQUIRED)
VITE_DOMAIN=https://yourdomain.com
VITE_API_BASE_URL=https://yourdomain.com/api

# Contact Configuration
VITE_CONTACT_EMAIL=contact@yourdomain.com
VITE_WHATSAPP_NUMBER=+919209252547

# Analytics (Optional but Recommended)
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_GOOGLE_TAG_MANAGER_ID=GTM-XXXXXXX
VITE_HOTJAR_ID=your_hotjar_id

# Security (Recommended for form protection)
VITE_RECAPTCHA_SITE_KEY=your_recaptcha_site_key_here

# Feature Flags (Optional)
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CHAT_WIDGET=true
